            <asp:TextBox CssClass="form-control" ID="txtToDatePicker" runat="server" />
            <asp:Button CssClass="form-control" runat="server" OnClick="btnSearchPopup_Click" Text="Buscar" />
        </div>


        <hr />
     <div id="printdivcontent">
<div id="divtable" class="table-responsive" runat="server">
   </div>
         </div>

        <asp:Label id="lblMessage" Visible="false" runat="server"></asp:Label>  
        <asp:Label id="lblTotalEvProgreso" runat="server"></asp:Label>  
        <br />
        <div class="pull-right">
        <asp:Button Id="BtnPrint" runat="server"  OnClientClick="PrintDiv()" CssClass="btn" Text="Imprimir"></asp:Button>
        <asp:Button Id="BtnExportExcel" runat="server" OnClick="BtnExportExcel_Click" CssClass="btn" Text="Exportar Excel"></asp:Button>
