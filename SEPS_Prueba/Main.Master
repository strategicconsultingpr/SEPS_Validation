    <script type='text/javascript' src="<%= Page.ResolveClientUrl("~/vendor/bootstrap-3.3.2-dist/js/bootstrap.min.js") %>"></script>
    <script>

        $(function () {
            var dateFormat = "yy/mm/dd",
                from = $("#txtFromDatePicker")
                    .datepicker({
                        minDate: new Date(1940,01,01),
                        yearRange: "1940:"+ new Date().getFullYear().toString(),
                        changeMonth: true,
                        changeYear: true,
                        numberOfMonths: 1

                    })
                    .on("change", function () {
                        to.datepicker("option", "minDate", getDate(this));
                    }),
                to = $("#txtToDatePicker").datepicker({
                    minDate: new Date(1940, 01, 01),
                    yearRange: "1940:" + new Date().getFullYear().toString(),
                    changeMonth: true,
                    changeYear: true,
                    numberOfMonths: 1
                })
                    .on("change", function () {
                        from.datepicker("option", "maxDate", getDate(this));
                    });

            function getDate(element) {
                var date;
                try {
                    date = $.datepicker.parseDate(dateFormat, element.value);
                } catch (error) {
                    date = null;
                }

                return date;
            }
        });

        function PrintDiv() {
            var d = new Date();
            var divContents = document.getElementById("printdivcontent").innerHTML;
            var nombrePrograma = document.getElementById("lblNbPrograma").textContent;

            var printWindow = window.open();
            printWindow.document.write('<html><head>');
            printWindow.document.write('</head><body style="font-family:courier;"><style>h2 {text-align: center;} a {text-decoration:none; color:black;}  table { font-size: 12px; width:100%; border-collapse:collapse; } table, td, th { border: 1px solid black;}</style>');
            printWindow.document.write('<h2>Sistema de Alerta de Evaluaciones de Progreso</h2>');
            printWindow.document.write('<p>Programa: ' + nombrePrograma + '</p>');
            printWindow.document.write('<p>Fecha: ' + d.toDateString() + '</p>');
            printWindow.document.write('</br>');
            printWindow.document.write(divContents);
            printWindow.document.write('</body></html>');
            printWindow.document.close();
            printWindow.print();
        }  

        $(document).ready(function ()
        {
            if (document.location.pathname != "/frmHome.aspx") { $("#BtnSaep1").hide(); $("#BtnSaep2").hide(); } else { $("#BtnSaep1").show(); $("#BtnSaep2").show();  }
        });
       
        function popup()
        {
            $('#myModal').modal('show');          
        }

        

       
    </script>
    <!--[if IE 10]>
        <script type='text/javascript' src= "<%= Page.ResolveClientUrl("~/vendor/ie10-viewport-bug-workaround.js") %>"></script>
    <![endif]-->
